<h1>Listing regapps</h1>

<table>
  <tr>
    <th>Cid</th>
    <th>Token</th>
    <th>Key</th>
  </tr>

<% @regapps.each do |regapp| %>
  <tr>
    <td><%=h regapp.cid %></td>
    <td><%=h regapp.token %></td>
    <td><%=h regapp.key %></td>
    <td><%= link_to 'Show', regapp %></td>
    <td><%= link_to 'Edit', edit_regapp_path(regapp) %></td>
    <td><%= link_to 'Destroy', regapp, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New regapp', new_regapp_path %>